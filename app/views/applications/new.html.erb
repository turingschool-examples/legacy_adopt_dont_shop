<h1>New Application</h1>
<%= form_tag "/applications" do %>
  <%= label_tag :applicant_name %>
  <%= text_field_tag :applicant_name %><br>
  <%= label_tag :applicant_address %>
  <%= text_field_tag :applicant_address %><br>
  <%= label_tag "City" %>
  <%= text_field_tag :city %><br>
  <%= label_tag "State" %>
  <%= text_field_tag :state %><br>
  <%= label_tag "Zip Code" %>
  <%= number_field_tag :zip %><br>
  <%= submit_tag 'Submit Application' %>
<% end %>
